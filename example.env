# Core agent settings (used by AgentConfig.from_env)
AGENT_MODEL_ENDPOINT=databricks-gpt-5-2
AGENT_UC_VOLUME_PATH=/Volumes/<catalog>/<schema>/<volume>
AGENT_SKILLS_DIR=.claude/skills
AGENT_LOCAL_OUTPUT_DIR=./output
AGENT_MAX_ITERATIONS=10

# Optional: set a fixed session id for debugging. Leave empty in normal usage.
# AGENT_SESSION_ID=

# Local development only.
# Leave empty in Databricks App / Model Serving runtime so platform auth is used.
DATABRICKS_CONFIG_PROFILE=

# Legacy aliases still supported for compatibility with existing app.yaml
SERVING_ENDPOINT_NAME=databricks-gpt-5-2
UC_VOLUME_PATH=/Volumes/<catalog>/<schema>/<volume>
SKILLS_DIR=.claude/skills

